<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".screenMain.ScreenLogin"
    android:background="@drawable/screenprincipalbackground"
    >

    <Button
        android:id="@+id/btn_enter"
        android:layout_width="189dp"
        android:layout_height="190dp"
        android:layout_marginHorizontal="10dp"
        android:layout_marginBottom="80dp"
        android:backgroundTint="#FFF1F1"
        android:elevation="2dp"
        android:paddingVertical="14dp"
        android:text="Enter"
        android:textAllCaps="true"
        android:textColor="#0041B9"
        android:textSize="40dp"
        android:textStyle="bold"
        app:cornerRadius="700dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.853"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#15AEF1"
        app:strokeColor="#549CE4"
        app:strokeWidth="3dp"
        tools:ignore="MissingConstraints"
        />

</androidx.constraintlayout.widget.ConstraintLayout>